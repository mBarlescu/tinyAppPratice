<!DOCTYPE html>
<html>
<head>
  <title> TinyApp</title>
</head>
<body>
  <% include ./partials/_header %>

  <ul>
    <% if(user_id) { %>
  <% for(const eachURL in urls) { %>
    <% if(urls[eachURL].id === user_id) { %>
    <li><%= eachURL %>: <a href="<%= urls[eachURL].longURL  %>"> <%= urls[eachURL].longURL %> </a></li>
    <% } %>
    <% if((user_id) && user_id === urls[eachURL].id){ %>
    <form method='POST' action='/urls/<%= eachURL %>/delete'>
      <a href="http://localhost:8080/urls/<%= eachURL %>"> Edit </a>
      <button>delete</button>
    </form>
    <% } %>
    <% } %>
    <% } %>
  </ul>


</body>
</html>